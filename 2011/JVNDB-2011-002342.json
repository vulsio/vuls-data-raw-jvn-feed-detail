					"cpe": {
						"text": "cpe:/a:vmware:springsource_spring_security",
						"version": "2.2"
					},
					"versionnumber": [
						"2.0.0 から 2.0.6",
						"3.0.0 から 3.0.5"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "6.8",
					"vector": "AV:N/AC:M/Au:N/C:P/I:P/A:P"
				}
			],
			"impactitem": {
				"description": "第三者により、(1) java.lang.Proxy インスタンスをシリアル化して InvocationHandler を使用する、または (2) java.lang.Runtime class を介してインターナル AOP インタフェースにアクセスすることで、セキュリティ制限を回避される、および信頼されないコードを実行される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
