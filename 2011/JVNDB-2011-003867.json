	"vulinfoid": "JVNDB-2011-003867",
	"vulinfodata": {
		"title": "ZOHO ManageEngine ADSelfService Plus における任意のユーザアカウントへのアクセスを取得される脆弱性",
		"vulinfodescription": {
			"overview": "ZOHO ManageEngine ADSelfService Plus には、ユーザのパスワードをリセットされ、結果として任意のユーザアカウントへのアクセスを取得される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "Zoho Corporation",
					"productname": "ManageEngine ADSelfService Plus",
					"cpe": {
						"text": "cpe:/a:zohocorp:manageengine_adselfservice_plus",
						"version": "2.2"
					},
					"versionnumber": [
						"4.5 Build 4500 未満"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "5",
					"vector": "AV:N/AC:L/Au:N/C:N/I:P/A:N"
				}
			],
			"impactitem": {
				"description": "第三者により、accounts/ValidateUser に対してユーザIDを提供され、その後 accounts/ResetResult に対して新しいパスワードを提供されることで、ユーザのパスワードをリセットされ、結果として任意のユーザアカウントへのアクセスを取得される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
