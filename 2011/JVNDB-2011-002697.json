          "productname": "Puppet Enterprise",
          "cpe": {
            "text": "cpe:/a:puppetlabs:puppet_enterprise",
            "version": "2.2"
          },
          "versionnumber": [
            "Users 1.0",
            "Users 1.1",
            "Users 1.2.4 未満の 1.2"
          ]
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
