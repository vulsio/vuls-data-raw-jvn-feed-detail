					"cpe": {
						"text": "cpe:/a:apache:tomcat",
						"version": "2.2"
					},
					"versionnumber": [
						"10.1.0-M1から10.1.24まで",
						"11.0.0-M1から11.0.0-M20まで",
						"9.0.13から9.0.89まで"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "3.0",
					"severity": {
						"text": "High",
						"type": "Base"
