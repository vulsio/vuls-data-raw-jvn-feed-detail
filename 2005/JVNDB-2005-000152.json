					},
					"versionnumber": [
						"5.51 およびそれ以前"
					]
				},
				{
					"name": "サン・マイクロシステムズ",
					"productname": "Sun Solaris",
					"cpe": {
						"text": "cpe:/o:sun:solaris",
						"version": "2.2"
					},
					"versionnumber": [
