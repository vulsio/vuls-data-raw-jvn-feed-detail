					"cpe": {
						"text": "cpe:/a:ss-proj:shirasagi",
						"version": "2.2"
					},
					"versionnumber": [
						"v1.14.4 から v1.15.0 (CVE-2022-43479)",
						"v1.16.2 より前のバージョン (CVE-2022-43499)"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
