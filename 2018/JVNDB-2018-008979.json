{
  "vulinfoid": "JVNDB-2018-008979",
  "vulinfodata": {
    "title": "Cisco ASA および FTD の SIP インスペクション機能におけるサービス運用妨害 (DoS) の脆弱性",
    "vulinfodescription": {
      "overview": "Cisco Adaptive Security Appliance (ASA) ソフトウェアおよび Cisco Firepower Threat Defense (FTD) ソフトウェアの SIP インスペクション機能には、SIP トラフィックの解析処理に問題があり、CPU 負荷が高くなってサービス運用妨害 (DoS) 状態になる可能性があります。"
    },
    "affected": {
      "affecteditem": [
        {
          "name": "シスコシステムズ",
          "productname": "Cisco Adaptive Security Appliance ソフトウェア",
          "cpe": {
            "text": "cpe:/a:cisco:adaptive_security_appliance_software",
            "version": "2.2"
          }
        },
        {
          "name": "シスコシステムズ",
          "productname": "Cisco Firepower Threat Defense ソフトウェア",
          "cpe": {
            "text": "cpe:/a:cisco:firepower_threat_defense_software",
            "version": "2.2"
          }
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "High",
            "type": "Base"
          },
          "base": "7.8",
          "vector": "AV:N/AC:L/Au:N/C:N/I:N/A:C"
        },
        {
