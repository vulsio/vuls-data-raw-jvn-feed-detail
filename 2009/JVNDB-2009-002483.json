	"vulinfoid": "JVNDB-2009-002483",
	"vulinfodata": {
		"title": "IBM DB2 の Relational Data Services コンポーネントにおけるパスワードの引数を取得される脆弱性",
		"vulinfodescription": {
			"overview": "IBM DB2 の Relational Data Services コンポーネントには、SET ENCRYPTION PASSWORD ステートメントからパスワードの引数を取得される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "IBM",
					"productname": "IBM DB2",
					"cpe": {
						"text": "cpe:/a:ibm:db2",
