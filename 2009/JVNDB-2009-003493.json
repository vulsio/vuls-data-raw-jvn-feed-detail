	"vulinfoid": "JVNDB-2009-003493",
	"vulinfodata": {
		"title": "Worldweaver DX Studio Player における任意のコマンドを実行される脆弱性",
		"vulinfodescription": {
			"overview": "Worldweaver DX Studio Player には、Firefox のプラグインとして使用される際、shell.execute JavaScript API メソッドへのアクセスを制限しないため、任意のコマンドを実行される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "dxstudio",
					"productname": "dx studio player",
					"cpe": {
						"text": "cpe:/a:dxstudio:dx_studio_player",
						"version": "2.2"
					},
					"versionnumber": [
						"3.0.29.0、3.0.22.0、3.0.12.0、および 3.0.29.1 未満のその他のバージョン"
					]
				},
				{
					"name": "Mozilla Foundation",
					"productname": "Mozilla Firefox",
					"cpe": {
						"text": "cpe:/a:mozilla:firefox",
						"version": "2.2"
					}
				}
			]
		},
		"impact": {
