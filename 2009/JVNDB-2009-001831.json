    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "High",
            "type": "Base"
          },
          "base": "7.5",
          "vector": "AV:N/AC:L/Au:N/C:P/I:P/A:P"
        }
      ],
      "impactitem": {
        "description": "巧妙に細工された第一引数の値を持つ apple.laf.CColourUIResource コンストラクタを呼び出すことにより、任意のコードを実行される可能性があります。"
      }
    },
