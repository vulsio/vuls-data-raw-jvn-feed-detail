					"productname": "Cisco PIX Firewall",
					"cpe": {
						"text": "cpe:/h:cisco:pix_firewall",
						"version": "2.2"
					},
					"versionnumber": [
						"5.2  ",
						"6.0  ",
						"6.1  ",
						"6.2  "
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "5",
					"vector": "AV:N/AC:L/Au:N/C:N/I:N/A:P"
				}
			],
			"impactitem": {
				"description": "SIP サービスがサービス運用妨害 (DoS) 状態となる、あるいは、SIP サービスを実行している権限で任意のコードを実行される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
				"description": "ベンダより正式な対策が公開されています。ベンダ情報を参照して適切な対策を実施してください。"
			}
