	"vulinfodata": {
		"title": "IBM Tivoli Endpoint Manager for Lifecycle Management の Endpoint Manager for Remote Control コンポーネントにおけるセッション Cookie をキャプチャされる脆弱性",
		"vulinfodescription": {
			"overview": "IBM Tivoli Endpoint Manager for Lifecycle Management の Endpoint Manager for Remote Control コンポーネントは、HTTPS セッションのセッション Cookie に secure フラグを設定しないため、セッション Cookie をキャプチャされる脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "IBM",
					"productname": "IBM Endpoint Manager family",
					"cpe": {
						"text": "cpe:/a:ibm:endpoint_manager_family",
						"version": "2.2"
					},
					"versionnumber": [
						"の Endpoint Manager for Remote Control 9.0.1 IF6 未満の 9.0.1",
						"の Endpoint Manager for Remote Control 9.1.0 IF6 未満の 9.1.0"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
