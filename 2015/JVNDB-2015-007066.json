          "cpe": {
            "text": "cpe:/a:redhat:satellite",
            "version": "2.2"
          },
          "versionnumber": [
            "6.1 for RHEL 6 x86_64",
            "6.1 for RHEL 7 x86_64",
            "Capsule 6.1 for RHEL 6 x86_64",
            "Capsule 6.1 for RHEL 7 x86_64"
          ]
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
