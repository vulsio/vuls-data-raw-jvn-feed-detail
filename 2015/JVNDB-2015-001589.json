    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Critical",
            "type": "Base"
          },
          "base": "9",
          "vector": "AV:N/AC:L/Au:S/C:C/I:C/A:C"
        }
      ],
      "impactitem": {
        "description": "リモート認証されたユーザにより、実行可能な拡張子を持つファイルをアップロードされることで、任意のコードを実行される可能性があります。"
      }
    },
    "solution": {
