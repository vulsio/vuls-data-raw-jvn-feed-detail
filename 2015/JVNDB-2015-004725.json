  "vulinfoid": "JVNDB-2015-004725",
  "vulinfodata": {
    "title": "WordPress 用 SecureMoz Security Audit プラグインの class/__functions.php における PHP オブジェクトインジェクション攻撃を実行される脆弱性",
    "vulinfodescription": {
      "overview": "WordPress 用 SecureMoz Security Audit プラグインの class/__functions.php の tweet_info 関数は、シリアル化されたデータのダウンロードに HTTPS セッションを使用しないため、PHP オブジェクトインジェクション攻撃を実行され、任意の PHP コードを実行される脆弱性が存在します。"
    },
    "affected": {
      "affecteditem": [
        {
          "name": "SecureMoz",
          "productname": "SecureMoz Security Audit",
          "cpe": {
            "text": "cpe:/a:securemoz:securemoz_security_audit",
            "version": "2.2"
          },
          "versionnumber": [
            "1.0.5 およびそれ以前"
          ]
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Medium",
            "type": "Base"
          },
          "base": "6.8",
          "vector": "AV:N/AC:M/Au:N/C:P/I:P/A:P"
        }
      ],
      "impactitem": {
        "description": "中間者攻撃 (man-in-the-middle attack) により、クライアント-サーバデータストリームを変更されることで、PHP オブジェクトインジェクション攻撃を実行され、任意の PHP コードを実行される可能性があります。"
      }
    },
    "solution": {
      "solutionitem": {
