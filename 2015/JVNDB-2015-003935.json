    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Medium",
            "type": "Base"
          },
          "base": "5",
          "vector": "AV:N/AC:L/Au:N/C:P/I:N/A:N"
        }
      ],
      "impactitem": {
        "description": "第三者により、クライアント-サーバデータストリームから USERACCT リクエストを削除されることで、アクセス制限を回避される可能性があります。"
      }
    },
    "solution": {
