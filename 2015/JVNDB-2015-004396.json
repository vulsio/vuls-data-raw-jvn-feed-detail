	"vulinfoid": "JVNDB-2015-004396",
	"vulinfodata": {
		"title": "Drupal 用 Views Bulk Operations モジュールにおけるユーザアカウントを編集される脆弱性",
		"vulinfodescription": {
			"overview": "Drupal 用 Views Bulk Operations (VBO) モジュールには、ロールを変更するための bulk オペレーションが有効になっている場合、ユーザアカウントを編集され、任意のロールをアカウントに追加される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "Views Bulk Operations project",
					"productname": "Views Bulk Operations",
					"cpe": {
						"text": "cpe:/a:views_bulk_operations_project:views_bulk_operations",
						"version": "2.2"
					},
					"versionnumber": [
						"6.x-1.x",
						"7.x-3.3 未満の 7.x-3.x"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "4.9",
					"vector": "AV:N/AC:M/Au:S/C:N/I:P/A:P"
				}
			],
			"impactitem": {
				"description": "リモート認証されたユーザにより、VBO が有効になっているビューを一覧表示しているユーザアカウントを介して、ユーザアカウントを編集され、任意のロールをアカウントに追加される可能性があります。"
			}
		},
