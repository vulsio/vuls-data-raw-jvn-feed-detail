          },
          "versionnumber": [
            "6.5.5 FP2 およびそれ以前",
            "7.0.2 およびそれ以前"
          ]
        },
        {
          "name": "IBM",
          "productname": "IBM Lotus Domino Web Access",
          "cpe": {
            "text": "cpe:/a:ibm:lotus_domino_web_access",
            "version": "2.2"
          },
