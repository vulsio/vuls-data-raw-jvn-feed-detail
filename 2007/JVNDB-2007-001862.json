      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "High",
            "type": "Base"
          },
          "base": "7.6",
          "vector": "AV:N/AC:H/Au:N/C:C/I:C/A:C"
        }
      ],
      "impactitem": {
        "description": "第三者により、quicktime.util.QTHandleRef 内の toQTPointer メソッドへのパラメータを介して、QTPointerRef を生成する際に任意のメモリを変更に利用される、および任意のコードを実行される可能性があります。"
      }
    },
    "solution": {
      "solutionitem": {
