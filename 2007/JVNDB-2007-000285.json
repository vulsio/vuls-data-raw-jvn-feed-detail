	"vulinfoid": "JVNDB-2007-000285",
	"vulinfodata": {
		"title": "Oracle Application Server の Oracle Discoverer サーブレットに関するサービス運用妨害 (DoS) の脆弱性",
		"vulinfodescription": {
			"overview": "Oracle Application Server には、Oracle Discoverer サーブレットを介して TNS STOP コマンドを送信することで、Oracle TNS Listener がシャットダウンする脆弱性が存在します。(AS01)"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "オラクル",
					"productname": "Oracle Application Server",
					"cpe": {
						"text": "cpe:/a:oracle:application_server",
						"version": "2.2"
					},
					"versionnumber": [
						"10.1.2.0.2",
						"10.1.2.2.0",
						"9.0.4.3"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "High",
						"type": "Base"
					},
					"base": "7.8",
					"vector": "AV:N/AC:L/Au:N/C:N/I:N/A:C"
				}
			],
			"impactitem": {
				"description": "第三者により TNS STOP コマンドを送信されることで Oracle TNS Listener がシャットダウンを起こし、結果としてサービス運用妨害 (DoS) 状態となる可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
