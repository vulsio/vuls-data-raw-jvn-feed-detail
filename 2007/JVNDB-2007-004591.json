	"vulinfoid": "JVNDB-2007-004591",
	"vulinfodata": {
		"title": "OneOrZero Helpdesk におけるクロスサイトスクリプティング (XSS) 攻撃を実行される脆弱性",
		"vulinfodescription": {
			"overview": "OneOrZero Helpdesk の common.php の stripScripts 関数には、不完全なブラックリストにより、クロスサイトスクリプティング (XSS) 攻撃を実行される、および任意の Web スクリプトまたは HTML を挿入される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "OneOrZero",
					"productname": "oneorzero helpdesk",
					"cpe": {
						"text": "cpe:/a:oneorzero:oneorzero_helpdesk",
						"version": "2.2"
					},
					"versionnumber": [
						"1.6.5.4、1.6.4.2 およびその他のバージョン"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "4.3",
					"vector": "AV:N/AC:M/Au:N/C:N/I:P/A:N"
				}
			],
			"impactitem": {
				"description": "第三者により、(1) tcreate.php または (2) tupdate.php への description パラメータの SCRIPT タグなしで XSS シーケンスを介して、クロスサイトスクリプティング (XSS) 攻撃を実行される、および任意の Web スクリプトまたは HTML を挿入される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
