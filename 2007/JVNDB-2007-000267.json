			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Critical",
						"type": "Base"
					},
					"base": "10",
					"vector": "AV:N/AC:L/Au:N/C:C/I:C/A:C"
				}
			],
			"impactitem": {
				"description": "第三者が細工した RPC パケットを処理した際に、任意のコードを実行される可能性があります。"
			}
		},
