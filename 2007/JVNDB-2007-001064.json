					},
					"versionnumber": [
						"7.3 未満のバージョン"
					]
				},
				{
					"name": "アップル",
					"productname": "Apple Mac OS X",
					"cpe": {
						"text": "cpe:/o:apple:mac_os_x",
						"version": "2.2"
					},
					"versionnumber": [
						"v10.4.11",
						"v10.5.2"
					]
				},
				{
					"name": "アップル",
					"productname": "Apple Mac OS X Server",
					"cpe": {
						"text": "cpe:/o:apple:mac_os_x_server",
						"version": "2.2"
					},
					"versionnumber": [
						"v10.4.11",
						"v10.5.2"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "6.4",
					"vector": "AV:N/AC:L/Au:N/C:P/I:N/A:P"
				}
			],
			"impactitem": {
				"description": "“\\X?\\d” および “\\P{L}?\\d” 形式の正規表現を UTF-8 以外のモードで使用した際に、重要な情報を奪取される、あるいはサービス運用妨害 (DoS) 状態にされる可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
				"description": "ベンダより正式な対策が公開されています。ベンダ情報を参照して適切な対策を実施してください。"
			}
		},
