          },
          "versionnumber": [
            "R7.2 およびそれ以前 (Xserver 1.3.0 およびそれ以前)"
          ]
        },
        {
          "name": "サン・マイクロシステムズ",
          "productname": "Sun Java Desktop System",
          "cpe": {
            "text": "cpe:/a:sun:java_desktop_system",
            "version": "2.2"
          },
          "versionnumber": [
            "release 2 (Solaris 9 x86 用)"
          ]
        },
        {
