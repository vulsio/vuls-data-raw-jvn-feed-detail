    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Medium",
            "type": "Base"
          },
          "base": "5",
          "vector": "AV:N/AC:L/Au:N/C:N/I:P/A:N"
        }
      ],
      "impactitem": {
        "description": "第三者により、(1) cart.php、または (2) index.php への \"ccSID\" で始まるクッキー名の CRLF シーケンスを介して、HTTP ヘッダを挿入される、または HTTPレスポンス分割攻撃を実行される可能性があります。"
      }
    },
    "solution": {
      "solutionitem": {
