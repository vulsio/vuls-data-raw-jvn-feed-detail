  "vulinfodata": {
    "title": "IBM Algo One の複数のコンポーネントにおける重要な情報を取得される脆弱性",
    "vulinfodescription": {
      "overview": "IBM Algo One の UDS の MetaData Management Tools、Algo Security Access Control Management の ACSWeb、および AlgoWebApps の ACSWeb には、ログインリクエストを暗号化しないため、重要な情報を取得される脆弱性が存在します。"
    },
    "affected": {
      "affecteditem": [
        {
          "name": "IBM",
          "productname": "IBM Algo One",
          "cpe": {
            "text": "cpe:/a:ibm:algo_one",
            "version": "2.2"
          },
          "versionnumber": [
            "の Algo Security Access Control Management 4.7.0 から 4.9.0",
            "の AlgoWebApps 5.0.0",
            "の UDS 4.7.0 から 5.0.0"
          ]
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
