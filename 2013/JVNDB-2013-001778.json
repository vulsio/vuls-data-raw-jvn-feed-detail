	"vulinfoid": "JVNDB-2013-001778",
	"vulinfodata": {
		"title": "EMC RSA Authentication Agent におけるパスコード要求を回避される脆弱性",
		"vulinfodescription": {
			"overview": "EMC RSA Authentication Agent は、Quick PIN Unlock タイムアウト機能を適切に適用しないため、スクリーンセーブされたセッションに対してのパスコード要求を回避される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "DELL EMC (旧 EMC Corporation)",
					"productname": "RSA SecurID Authentication Agent",
					"cpe": {
						"text": "cpe:/a:emc:rsa_authentication_agent",
						"version": "2.2"
					},
					"versionnumber": [
						"7.1.2 未満の 7.1.x (Windows)"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "5.4",
					"vector": "AV:A/AC:M/Au:N/C:P/I:P/A:P"
				}
			],
			"impactitem": {
				"description": "攻撃者により、タイムアウト後に PIN を入力されることで、スクリーンセーブされたセッションに対してのパスコード要求を回避される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
