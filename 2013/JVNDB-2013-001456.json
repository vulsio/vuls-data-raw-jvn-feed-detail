	"vulinfoid": "JVNDB-2013-001456",
	"vulinfodata": {
		"title": "Rack の Rack::Session::Cookie におけるセッション cookie を推測される脆弱性",
		"vulinfodescription": {
			"overview": "Rack の Rack::Session::Cookie には、セッション cookie を推測される、権限を取得される、および任意のコードを実行される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "Rack",
					"productname": "Rack",
					"cpe": {
						"text": "cpe:/a:rack_project:rack",
						"version": "2.2"
					},
					"versionnumber": [
						"1.1.6 未満の 1.1.x",
						"1.2.8 未満の 1.2.x",
						"1.3.10 未満の 1.3.x",
						"1.4.5 未満の 1.4.x",
						"1.5.2 未満の 1.5.x"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "5.1",
					"vector": "AV:N/AC:H/Au:N/C:P/I:P/A:P"
				}
			],
			"impactitem": {
				"description": "第三者により、一定の時間において実行されない HMAC 比較機能に関連したタイミング攻撃を介して、セッション cookie を推測される、権限を取得される、および任意のコードを実行される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
				"description": "ベンダより正式な対策が公開されています。ベンダ情報を参照して適切な対策を実施してください。"
			}
		},
