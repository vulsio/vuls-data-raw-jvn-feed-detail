				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "6.5",
					"vector": "AV:N/AC:L/Au:S/C:P/I:P/A:P"
				}
			],
			"impactitem": {
				"description": "\"access CiviCRM\" パーミッションを持つ、リモート認証されたユーザにより、アクセス制限を回避される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
