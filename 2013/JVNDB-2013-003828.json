	"vulinfoid": "JVNDB-2013-003828",
	"vulinfodata": {
		"title": "Apache Santuario XML Security for Java における XML 署名を偽装される脆弱性",
		"vulinfodescription": {
			"overview": "Apache Santuario XML Security for Java の jcp/xml/dsig/internal/dom/DOMCanonicalizationMethod.java には、XML 署名を偽装される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "Apache Software Foundation",
					"productname": "Apache Santuario XML Security for Java",
					"cpe": {
						"text": "cpe:/a:apache:xml_security_for_java",
						"version": "2.2"
					},
					"versionnumber": [
						"1.4.8 未満の 1.4.x",
						"1.5.5 未満の 1.5.x"
					]
				},
				{
					"name": "オラクル",
					"productname": "Oracle Fusion Middleware",
					"cpe": {
						"text": "cpe:/a:oracle:fusion_middleware",
						"version": "2.2"
					},
					"versionnumber": [
						"の GlassFish Communications Server 2.0"
					]
				},
				{
					"name": "オラクル",
					"productname": "Oracle GlassFish Server",
					"cpe": {
						"text": "cpe:/a:oracle:glassfish_server",
						"version": "2.2"
					},
					"versionnumber": [
						"2.1.1",
						"3.0.1",
						"3.1.2"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "4.3",
					"vector": "AV:N/AC:M/Au:N/C:N/I:P/A:N"
				}
			],
			"impactitem": {
				"description": "攻撃者により、任意の脆弱な \"署名の SignedInfo パートに適用する正規化アルゴリズム (canonicalization algorithm to apply to the SignedInfo part of the Signature)\" を設定する CanonicalizationMethod パラメータを使用されることで、XML 署名を偽装される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
				"description": "ベンダより正式な対策が公開されています。ベンダ情報を参照して適切な対策を実施してください。"
			}
		},
