{
  "vulinfoid": "JVNDB-2013-001572",
  "vulinfodata": {
    "title": "OpenStack Compute Folsom および Essex における他のユーザのボリュームからブートされる脆弱性",
    "vulinfodescription": {
      "overview": "OpenStack Compute (Nova) Folsom および Essex の VM 起動 (boot-from-volume) 機能には、nova-volume を使用している場合、他のユーザのボリュームからブートされる脆弱性が存在します。"
    },
    "affected": {
      "affecteditem": [
        {
          "name": "Canonical",
          "productname": "Ubuntu",
          "cpe": {
            "text": "cpe:/o:canonical:ubuntu",
            "version": "2.2"
          },
          "versionnumber": [
            "11.10",
            "12.04 LTS",
            "12.10"
          ]
        },
        {
          "name": "OpenStack",
          "productname": "OpenStack Compute Folsom",
          "cpe": {
            "text": "cpe:/a:openstack:folsom",
            "version": "2.2"
          }
        },
        {
          "name": "OpenStack",
          "productname": "OpenStack Essex",
          "cpe": {
            "text": "cpe:/a:openstack:essex",
            "version": "2.2"
          }
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Medium",
            "type": "Base"
          },
          "base": "6.5",
          "vector": "AV:N/AC:L/Au:S/C:P/I:P/A:P"
        }
      ],
      "impactitem": {
        "description": "リモート認証されたユーザにより、block_device_mapping パラメータのボリューム ID を介して、他のユーザのボリュームからブートされる可能性があります。"
      }
    },
    "solution": {
      "solutionitem": {
        "description": "ベンダ情報および参考情報を参照して適切な対策を実施してください。"
      }
    },
