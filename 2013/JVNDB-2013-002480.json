	"vulinfoid": "JVNDB-2013-002480",
	"vulinfodata": {
		"title": "libxml2 におけるサービス運用妨害 (DoS) の脆弱性",
		"vulinfodescription": {
			"overview": "libxml2 には、解放済みメモリの使用 (Use-after-free) により、サービス運用妨害 (クラッシュ) 状態になる、および任意のコードを実行される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "xmlsoft.org",
					"productname": "libxml2",
					"cpe": {
						"text": "cpe:/a:xmlsoft:libxml2",
						"version": "2.2"
					},
					"versionnumber": [
						"2.9.0"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "High",
						"type": "Base"
					},
					"base": "7.5",
					"vector": "AV:N/AC:L/Au:N/C:P/I:P/A:P"
				}
			],
			"impactitem": {
				"description": "攻撃者により、(1) htmlParseChunk および (2) xmldecl_done 関数を介して、サービス運用妨害 (クラッシュ) 状態にされる、および任意のコードを実行される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
				"description": "ベンダより正式な対策が公開されています。ベンダ情報を参照して適切な対策を実施してください。"
			}
