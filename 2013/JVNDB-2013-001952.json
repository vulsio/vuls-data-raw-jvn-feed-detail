    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Low",
            "type": "Base"
          },
          "base": "2.1",
          "vector": "AV:N/AC:H/Au:S/C:N/I:P/A:N"
        }
      ],
      "impactitem": {
        "description": "\"administer user relationships\" 権限を持つリモート認証されたユーザにより、リレーションシップ名 (relationship name) を介して、任意の Web スクリプトまたは HTML を挿入される可能性があります。"
      }
    },
    "solution": {
      "solutionitem": {
