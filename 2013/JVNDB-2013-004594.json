					},
					"versionnumber": [
						"7.0",
						"7.1 SP2 MP1.1 v7 ロールアップ未満の 7.1"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
