  "vulinfoid": "JVNDB-2006-003048",
  "vulinfodata": {
    "title": "SQL-Ledger におけるログイン済みユーザとしてアクセス権を取得される脆弱性",
    "vulinfodescription": {
      "overview": "SQL-Ledger は、sessionid パラメータの値と sql-ledger-[username] クッキーの値が一致することでユーザを認証するため、ログイン済みユーザとしてアクセス権を取得される脆弱性が存在します。"
    },
    "affected": {
      "affecteditem": [
        {
          "name": "sql-ledger",
          "productname": "sql-ledger",
          "cpe": {
            "text": "cpe:/a:sql-ledger:sql-ledger",
