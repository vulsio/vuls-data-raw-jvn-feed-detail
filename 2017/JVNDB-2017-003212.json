  "vulinfodata": {
    "title": "SAP AS JAVA SSO Authentication Library におけるサービス運用妨害 (DoS) の脆弱性",
    "vulinfodescription": {
      "overview": "SAP AS JAVA SSO Authentication Library には、サービス運用妨害 (メモリ消費) 状態にされる脆弱性が存在します。  ベンダは、本脆弱性を SAP Security Note 2389042 として公開しています。"
    },
    "affected": {
      "affecteditem": [
        {
          "name": "SAP",
          "productname": "SAP AS JAVA SSO Authentication Library",
          "cpe": {
            "text": "cpe:/a:sap:sso_authentication_library",
            "version": "2.2"
          },
          "versionnumber": [
            "2.0 から 3.0"
          ]
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Medium",
            "type": "Base"
          },
          "base": "5",
          "vector": "AV:N/AC:L/Au:N/C:N/I:N/A:P"
        },
        {
          "version": "3.0",
          "severity": {
            "text": "High",
            "type": "Base"
          },
          "base": "7.5",
          "vector": "CVSS:3.0/AV:N/AC:L/PR:N/UI:N/S:U/C:N/I:N/A:H"
        }
      ],
      "impactitem": {
        "description": "リモートの攻撃者により、otp_logon_ui_resources/qr の width および height パラメータの過度に大きな値を介して、サービス運用妨害 (メモリ消費) 状態にされる可能性があります。"
      }
    },
