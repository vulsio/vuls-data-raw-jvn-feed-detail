      "affecteditem": [
        {
          "name": "ISC, Inc.",
          "productname": "BIND",
          "cpe": {
            "text": "cpe:/a:isc:bind",
            "version": "2.2"
          },
          "versionnumber": [
            "9.10.0 から 9.10.5-P1 まで",
            "9.10.5-S1 から 9.10.5-S2 まで",
            "9.11.0 から 9.11.1-P1 まで",
            "9.4.0 から 9.8.8 まで",
            "9.9.0 から 9.9.10-P1 まで",
            "9.9.3-S1 から 9.9.10-S2 まで"
          ]
        }
      ]
    },
