	"vulinfoid": "JVNDB-2014-005821",
	"vulinfodata": {
		"title": "Plex Media Server における Web サーバのホワイトリストを回避される脆弱性",
		"vulinfodescription": {
			"overview": "Plex Media Server には、Web サーバのホワイトリストを回避される、SSRF 攻撃を実行される、および任意の管理アクションを実行される脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "Plex, Inc.",
					"productname": "Plex Media Server",
					"cpe": {
						"text": "cpe:/a:plex:plex_media_server",
						"version": "2.2"
					},
					"versionnumber": [
						"0.9.9.3 未満"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "High",
						"type": "Base"
					},
					"base": "7.5",
					"vector": "AV:N/AC:L/Au:N/C:P/I:P/A:P"
				}
			],
			"impactitem": {
				"description": "第三者により、system/proxy の巧妙に細工された X-Plex-Url ヘッダを介して、バックエンド Web サーバのリクエストハンドラにより一貫性のない処理をされることで、Web サーバのホワイトリストを回避される、SSRF 攻撃を実行される、および任意の管理アクションを実行される可能性があります。"
			}
		},
		"solution": {
			"solutionitem": {
				"description": "ベンダより正式な対策が公開されています。ベンダ情報を参照して適切な対策を実施してください。"
			}
		},
