    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Medium",
            "type": "Base"
          },
          "base": "6.9",
          "vector": "AV:L/AC:M/Au:N/C:C/I:C/A:C"
        }
      ],
      "impactitem": {
        "description": "ローカルユーザにより、(1) setuid プロセスまたは (2) pkexec プロセスを介して、PolkitUnixProcess PolkitSubjec の競合状態を利用されることで、アクセス制限を回避される可能性があります。"
      }
    },
