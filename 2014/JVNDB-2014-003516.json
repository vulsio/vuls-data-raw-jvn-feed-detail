    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "High",
            "type": "Base"
          },
          "base": "7.5",
          "vector": "AV:N/AC:L/Au:N/C:P/I:P/A:P"
        }
      ],
      "impactitem": {
        "description": "第三者により、(1) blame、(2) file、または (3) stats ページへのリクエストの URI 内のファイル名のシェルのメタ文字を介して、任意のコマンドを実行される可能性があります。"
      }
    },
    "solution": {
