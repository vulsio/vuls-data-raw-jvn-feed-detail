	"vulinfoid": "JVNDB-2014-002648",
	"vulinfodata": {
		"title": "Cisco TelePresence System における重要なディレクトリ情報を取得される脆弱性",
		"vulinfodescription": {
			"overview": "Cisco TelePresence System (CTS) には、特定の HTTPS セッションが確立されない場合、HTTP にフォールバックし、重要なディレクトリ情報を取得される脆弱性が存在します。  ベンダは、本脆弱性を Bug ID CSCuj26326 として公開しています。 "
		},
		"affected": {
			"affecteditem": [
				{
					"name": "シスコシステムズ",
					"productname": "Cisco TelePresence System Software",
					"cpe": {
						"text": "cpe:/a:cisco:telepresence_system_software",
						"version": "2.2"
					},
					"versionnumber": [
						"6.0(.5)(5) およびそれ以前"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
					},
					"base": "4.3",
					"vector": "AV:N/AC:M/Au:N/C:P/I:N/A:N"
				}
			],
			"impactitem": {
				"description": "中間者攻撃 (man-in-the-middle attack) により、HTTPS トラフィックをブロックし、CTS と Cisco Unified Communications Manager (UCM) 間のネットワークポジションを利用することで、重要なディレクトリ情報を取得される可能性があります。"
			}
		},
