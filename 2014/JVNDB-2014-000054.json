					},
					"versionnumber": [
						"バージョン 3.2.0 から 3.2.8",
						"バージョン 4.0.0 から 4.0.4"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "Medium",
						"type": "Base"
