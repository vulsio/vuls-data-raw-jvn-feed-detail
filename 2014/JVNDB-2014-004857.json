	"vulinfodata": {
		"title": "Juniper JunosE におけるサービス運用妨害 (DoS) の脆弱性",
		"vulinfodescription": {
			"overview": "Juniper JunosE には、DEBUG severity icmpTraffic ロギング が有効になっている場合、サービス運用妨害 (SRP リセット) 状態にされる脆弱性が存在します。"
		},
		"affected": {
			"affecteditem": [
				{
					"name": "ジュニパーネットワークス",
					"productname": "JUNOSe",
					"cpe": {
						"text": "cpe:/o:juniper:junos_e",
						"version": "2.2"
					},
					"versionnumber": [
						"13.3.3p0-1 未満",
						"14.3.2 未満の 14.x",
						"15.1.0 未満の 15.x"
					]
				}
			]
		},
		"impact": {
			"cvss": [
				{
					"version": "2.0",
					"severity": {
						"text": "High",
						"type": "Base"
					},
					"base": "7.8",
					"vector": "AV:N/AC:L/Au:N/C:N/I:N/A:C"
				}
			],
			"impactitem": {
				"description": "第三者により、(1) インターフェース または (2) ループバック IP アドレスへの巧妙に細工された ICMP パケットを介して、ip_RxData_8 のプロセッサの例外を誘発されることで、サービス運用妨害 (SRP リセット) 状態にされる可能性があります。"
			}
		},
