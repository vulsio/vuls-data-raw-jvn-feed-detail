    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "High",
            "type": "Base"
          },
          "base": "8.5",
          "vector": "AV:N/AC:M/Au:S/C:C/I:C/A:C"
        }
      ],
      "impactitem": {
        "description": "manage_options および upload_files 権限を持つリモート認証されたユーザにより、PHP スクリプトをアップロードされる、および active_plugins へスクリプトのパス名を追加されることで、任意のコードを実行される可能性があります。"
      }
    },
    "solution": {
