    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "High",
            "type": "Base"
          },
          "base": "7.5",
          "vector": "AV:N/AC:L/Au:N/C:P/I:P/A:P"
        }
      ],
      "impactitem": {
        "description": "jtl.properties の JTLRMIREGISTRYSERVERS にカンマ区切りで複数のサーバ識別子を指定することで、リモートサーバにより、クライアントのサブミッションを操作され、任意のデータベースを変更される可能性があります。"
      }
    },
