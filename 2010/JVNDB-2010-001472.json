          },
          "versionnumber": [
            "2003 ",
            "2007 ",
            "xp "
          ]
        },
        {
          "name": "マイクロソフト",
          "productname": "Microsoft Visual Basic",
          "cpe": {
            "text": "cpe:/a:microsoft:visual_basic",
            "version": "2.2"
          },
          "versionnumber": [
            "for Applications",
            "for Applications SDK"
          ]
        }
      ]
    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Critical",
            "type": "Base"
          },
          "base": "9.3",
          "vector": "AV:N/AC:M/Au:N/C:C/I:C/A:C"
        }
      ],
      "impactitem": {
        "description": "第三者により、巧妙に細工されたドキュメントを介して、任意のコードを実行される可能性があります。"
      }
    },
    "solution": {
      "solutionitem": {
        "description": "ベンダより正式な対策が公開されています。ベンダ情報を参照して適切な対策を実施してください。"
      }
    },
