          },
          "versionnumber": [
            "build 8046 未満の 5.4、および build 8234 未満の 6.0"
          ]
        },
        {
          "name": "likewise",
          "productname": "likewise open",
          "cpe": {
            "text": "cpe:/a:likewise:likewise_open",
