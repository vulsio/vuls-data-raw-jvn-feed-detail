    },
    "impact": {
      "cvss": [
        {
          "version": "2.0",
          "severity": {
            "text": "Critical",
            "type": "Base"
          },
          "base": "10",
          "vector": "AV:N/AC:L/Au:N/C:C/I:C/A:C"
        }
      ],
      "impactitem": {
        "description": "第三者により、(1) HandleUpgradeAll、(2) AgentUpgrade、(3) HandleQueryNodeInfoReq、(4) HandleUpgradeTrace TCP パケット内に過度に長いパラメータを介して、任意のコードを実行される可能性があります。"
      }
    },
    "solution": {
